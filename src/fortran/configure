CONFIG_FILES=\${CONFIG_FILES-"Makefile src/Makefile src/farg.f \
	  include/mpif.h include/mpi_fortdefs.h src/mpif77 src/mpif77.conf \
	  src/mpif90 src/mpif90.conf src/initfcmn.f src/initfdte.f "}
